@{
    ViewData["Title"] = "Restaurants";
}

<h1>Restaurants</h1>
<a href="/restaurants/add"><button>Add more restaurants</button></a>

<div class="restaurantTable">
    @foreach(var restaurant in Model)
    {
        <div class="restaurantCell">@restaurant.RestaurantName</div>
        <div class="restaurantCell">@restaurant.GetCuisines()[0].CuisinesType</div>
    }
</div>

<a href="/"><button>Back</button></a>